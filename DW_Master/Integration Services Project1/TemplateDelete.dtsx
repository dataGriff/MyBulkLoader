<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{06512A11-68E0-4C30-B877-C32CFDD9751F}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="DECLARE @objectname NVARCHAR(MAX) = 'ds1_sc1_**prov**';&#xA;SET @objectname = REPLACE(SUBSTRING(@objectname, CHARINDEX('**',@OBJECTNAME),100),'**','')&#xA;&#xA;DECLARE @sql NVARCHAR(MAX) = '&#xA;BEGIN TRANSACTION&#xA;&#xA;	  IF OBJECT_ID(''tempdb..#'+@objectname+'_deletes'') IS NOT NULL&#xA;			DROP TABLE #'+@objectname+'_deletes; &#xA;	  CREATE TABLE #'+@objectname+'_deletes (KEYFIELDSXML VARCHAR(MAX))&#xA;	  &#xA;		INSERT INTO #'+@objectname+'_deletes &#xA;		SELECT KEYFIELDSXML FROM (&#xA;	  UPDATE dw_stage.can.['+@objectname+'_KeyFields]&#xA;	  SET PersistedIsRowCurrent=0&#xA;	   OUTPUT inserted.KeyFieldsXML&#xA;	  FROM can.['+@objectname+'_KeyFields] i&#xA;	  JOIN can.test_keyfieldsxml x ON&#xA;	  x.KeyFieldsXML = i.KeyFieldsXML&#xA;	  WHERE ActionDate IS NULL) AS T&#xA;	  &#xA;	  UPDATE can.test_keyfieldsxml&#xA;	  SET ActionDate = GETDATE()&#xA;	   FROM can.['+@objectname+'_KeyFields] i&#xA;	  JOIN can.test_keyfieldsxml x ON&#xA;	  x.KeyFieldsXML = i.KeyFieldsXML&#xA;	  WHERE ActionDate IS NULL&#xA;  &#xA;COMMIT TRANSACTION'&#xA;&#xA;EXEC sp_executesql @sql;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
